#include <iostream>
using namespace std;
int main(){
    string a = "Archit Deshpande";
    for (int i = 0; i < a.length(); i++){
        cout << a[i] << "\n";
    }
    for (char c : a){
        cout << c << "\n";
    }
    return 0;
}
#include<stdio.h>
struct student
{
	int rno;
	char name[20];
	int div;
};
void accept(struct student s[20],int n)
	{
		for(int i=0; i<n; i++)
		{
			printf("Enter the roll no\n");
			scanf("%d",&s[i].rno);
			printf("Enter the name\n");
			scanf("%s",s[i].name);
			printf("Enter the div\n");
			scanf("%d",&s[i].div);

		}
	}
void display(struct student s[20],int n)
{
	printf("Roll no. Name\t Div\n");
    for(int i=0;i<n;i++)
    {
    	printf("%d \t %s \t %d \n", s[i].rno,s[i].name,s[i].div);
    }
}
void bubblesort(struct student s[20],int n)
{
	struct student temp;
	for(int i=0;i<n-1;i++){
		for(int j=0;j<n-i-1;j++){
			if(s[j].rno>s[j+1].rno)
			{
				temp=s[j];
				s[j]= s[j+1];
				s[j+1]= temp;
			}
		}
	}
}
int main()
{
	struct student s[20];
	int n;
	printf("Enter no of students");
	scanf("%d",&n);
	accept(s,n);
	bubblesort(s,n);
	display(s,n);

}
